{
	"name": "build-your-own-react",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"keywords": [
		"build your own react"
	],
	"description": "build your own react",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"test": "vitest",
		"test:run": "vitest run",
		"test:coverage": "vitest run --coverage",
		"test:ui": "vitest --ui",
		"lint": "eslint . --ext .ts,.tsx",
		"lint:fix": "eslint . --ext .ts,.tsx --fix",
		"format": "prettier --write \"packages/**/*.{ts,tsx,json,md}\"",
		"format:check": "prettier --check \"packages/**/*.{ts,tsx,json,md}\"",
		"typecheck": "tsc --noEmit",
		"prepare": "node .husky/install.mjs"
	},
	"author": "lihzsky",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git@github.com:lihaizhong/build-your-own-react.git"
	},
	"devDependencies": {
		"@commitlint/cli": "^20.4.2",
		"@commitlint/config-conventional": "^20.4.2",
		"@eslint/js": "^10.0.1",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.2",
		"@vitejs/plugin-react": "^5.1.4",
		"@vitest/coverage-v8": "^4.0.18",
		"@vitest/ui": "^4.0.18",
		"commitlint": "^20.4.2",
		"eslint": "^10.0.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.5",
		"globals": "^17.3.0",
		"husky": "^9.1.7",
		"jsdom": "^28.1.0",
		"prettier": "^3.8.1",
		"rimraf": "^6.1.3",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.56.0",
		"vite": "^7.3.1",
		"vitest": "^4.0.18"
	}
}
